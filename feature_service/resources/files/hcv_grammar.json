            "reference": "@lab_creatinine @lab_glucose"
        },
        "panel_cmp_metabolic": {
            "tui": "T034",
            "sty": "Laboratory or Test Result",
            "codingScheme": "LNC",
            "text": "CMP Comprehensive (+BMP) : Creatinine, Glucose, Albumin, PT(Protein Total), ALT/AST/ALP(Alk Phos), Bilirubin",
            "reference": "@lab_creatinine @lab_glucose @lab_total_prot @lab_albumin @lab_alt @lab_ast  @lab_alk_phos"
        },
        "panel_lft_liver": {
            "tui": "T034",
            "sty": "Laboratory or Test Result",
            "codingScheme": "LNC",
            "text": "LFT Liver Function (C0023901): Albumin, PT,ALT/AST/ALP, Bilirubin, *Prothrombin time*, *INR*",
            "reference": "@lab_total_prot @lab_albumin @lab_albumin @lab_alt @lab_ast  @lab_alk_phos @lab_protime_total @lab_inr"
        },
        "lab_wbc": {
            "tui": "T034",
            "sty": "Laboratory or Test Result",
            "codingScheme": "LNC",
            "text": "WBC: White Blood Cell count. This test measures the total number of white blood cells (WBCs). There are 5 main types of WBCs: neutrophils, eosinophils, basophils, monocytes, and lymphocytes. Each type does a slightly different job. All of them help fight infection.",
            "reference": "https://www.hepatitis.va.gov/patient/hcv/diagnosis/labtests-WBC.asp"
        },
        "lab_anc": {
            "tui": "T034",
            "sty": "Laboratory or Test Result",
            "codingScheme": "LNC",
            "text": "ANC: Absolute Neutrophil Count. Neutrophils play a key role in inflammation, formation of pus, and destruction of bacteria",
            "reference": "https://www.hepatitis.va.gov/patient/hcv/diagnosis/labtests-neutrophils.asp"
        },
        "lab_hgb": {
            "tui": "T034",
            "sty": "Laboratory or Test Result",
            "codingScheme": "LNC",
            "text": "HBG: Hemoglobin A1C (The hemoglobin level is related to the hematocrit, red blood cell counts)",
            "reference": "https://www.hepatitis.va.gov/patient/hcv/diagnosis/labtests-hemoglobin.asp"
        },
        "lab_hct": {
            "tui": "T034",
            "sty": "Laboratory or Test Result",
            "codingScheme": "LNC",
            "text": "HCT: Hematocrit (red blood cell count)",
            "reference": "https://www.hepatitis.va.gov/patient/hcv/diagnosis/labtests-hematocrit.asp"
        },
        "lab_platelets": {
            "tui": "T034",
            "sty": "Laboratory or Test Result",
            "codingScheme": "LNC",
            "text": "Platlets: count of platlets per cubic millimeter (cmm) of whole blood. For patients with hepatitis C, having a low blood platelet count is a frequent complication associated with advanced disease.",
            "reference": "https://www.hepatitis.va.gov/patient/hcv/diagnosis/labtests-platelets.asp"
        },
        "lab_creatinine": {
            "tui": "T034",
            "sty": "Laboratory or Test Result",
            "codingScheme": "LNC",
            "text": "Creatinine is a waste product in your blood. It comes from protein in your diet and the normal breakdown of muscles of your body. Creatinine is removed from blood by the kidneys and then passes out of the body in your urine. If you have kidney disease, the level of creatinine in your blood increases.",
            "reference": "https://www.hepatitis.va.gov/patient/hcv/diagnosis/labtests-creatinine.asp"
        },
        "lab_glucose": {
            "tui": "T034",
            "sty": "Laboratory or Test Result",
            "codingScheme": "LNC",
            "text": "A type of sugar; the chief source of energy for living organisms. Measurement of the glucose in a biological specimen.",
            "reference": "https://www.hepatitis.va.gov/patient/hcv/diagnosis/labtests-glucose.asp"
        },
        "lab_protime": {
            "tui": "T034",
            "sty": "Laboratory or Test Result",
            "codingScheme": "LNC",
            "text": "Protime: Prothrombin time assay. (Prothrombin is a protein made by the liver. Prothrombin helps blood to make normal clots. The \"prothrombin time\" (PT) is one way of measuring how long it takes blood to form a clot, and it is measured in seconds)",
            "reference": "https://www.hepatitis.va.gov/patient/hcv/diagnosis/labtests-prothrombin-time.aspx"
        },
        "lab_inr": {
            "tui": "T034",
            "sty": "Laboratory or Test Result",
            "codingScheme": "LNC",
            "text": "INR: International Normalized Ratio, used to report blood coagulation test",
            "reference": "https://www.hepatitis.va.gov/patient/hcv/diagnosis/labtests-INR.asp"
        },
        "lab_albumin": {
            "tui": "T034",
            "sty": "Laboratory or Test Result",
            "codingScheme": "LNC",
            "text": "Albumin measurement: Albumin is a protein made by the liver. Albumin prevents fluid from leaking out of blood vessels into tissues",
            "reference": "https://www.hepatitis.va.gov/patient/hcv/diagnosis/labtests-albumin.asp"
        },
        "lab_alt": {
            "tui": "T034",
            "sty": "Laboratory or Test Result",
            "codingScheme": "LNC",
            "text": "ALT: Alanine aminotransferase measurement (liver enzymes)",
            "reference": "https://www.hepatitis.va.gov/patient/hcv/diagnosis/labtests-liver-enzymes.asp"
        },
        "lab_ast": {
            "tui": "T034",
            "sty": "Laboratory or Test Result",
            "codingScheme": "LNC",
            "text": "AST: Aspartate aminotransferase measurement (liver enzmes)",
            "reference": "https://www.hepatitis.va.gov/patient/hcv/diagnosis/labtests-liver-enzymes.asp"
        },
        "lab_alk_phos": {
            "tui": "T034",
            "sty": "Laboratory or Test Result",
            "codingScheme": "LNC",
            "text": "ALK: Alkaline phosphatase measurement. ALK is an enzyme made in liver cells and bile ducts. The alk phos level is a common test that is usually included when liver tests are performed as a group.",
            "reference": "https://www.hepatitis.va.gov/patient/hcv/diagnosis/labtests-alkaline-phosphatase.asp"
        },
        "lab_t_bili": {
            "tui": "T034",
            "sty": "Laboratory or Test Result",
            "codingScheme": "LNC",
            "text": "Bilirubin (total) measurement, a yellow substance that is part of bile. It is formed when your red blood cells break down. Too much bilirubin in the blood can cause jaundice. There is also a urine test for bilirubin.",
            "reference": "https://www.hepatitis.va.gov/patient/hcv/diagnosis/labtests-bilirubin.asp"
        },
        "lab_direct_bili": {
            "tui": "T034",
            "sty": "Laboratory or Test Result",
            "codingScheme": "LNC",
            "text": "Bilirubin (direct) measurement",
            "reference": "https://www.hepatitis.va.gov/patient/hcv/diagnosis/labtests-liver-panel.asp"
        },
        "lab_total_prot": {
            "tui": "T034",
            "sty": "Laboratory or Test Result",
            "codingScheme": "LNC",
            "text": "Total Protein: level is a measure of a number of different proteins in the blood. Total protein can be divided into the albumin and globulin fractions.",
            "reference": "https://www.hepatitis.va.gov/patient/hcv/diagnosis/labtests-total-protein.asp"
        }
    },
